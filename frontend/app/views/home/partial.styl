.hme{

  .dashboard{
    position: absolute;
    left: 70px;
    top: 10px;
    pointer-events:none;
  }
  .dashboard *{
    pointer-events: auto;
  }

  #map {
    display : block; 
    opacity : 1;
    position: absolute;
    top     : 0px;
    right   : 0px; 
    width   : 100%; 
    height  : 100%;
  }
}


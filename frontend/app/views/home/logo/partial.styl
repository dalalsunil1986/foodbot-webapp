@import 'nib'

.hme-logo {
  position: relative;
  margin: 22px 4px;
  width: 336px;
  height: 87px;
  border-radius: 70px;
  background-color: #fff;
  box-shadow: 0 1px 2px #fff, 0 -1px 1px rgba(102,102,102,0.65), inset 0px 0px 10px 0px rgba(171,171,171,0.56), inset 0px 0px 10px 0px #ababab;
  /*top internal highlight*/
}
.logo{
  z-index: 10;
  padding: 14px 26px;
} 

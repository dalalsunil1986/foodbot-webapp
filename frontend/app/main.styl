@import 'nib'

body{
  font-family: 'Open Sans', sans-serif;
  position: fixed;
  width: 100%;
  height: 100%;
}

a {
  color:#444444;
}
.container {
  width: 1170px;
  max-width: none !important;
}
map, div[map] {
  display : block; 
  opacity : 1;
  position: absolute;
  top     : 0px;
  right   : 0px; 
  width   : 100%; 
  height  : 100%;
}

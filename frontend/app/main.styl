@import 'nib'

.tag {
  float   : right;
  margin  : 0 4px 0 4px;
  opacity : .5;
  position: relative;
  height  : 17px; 
}

a {
  color:#444444;
}
.container {
  width: 1170px;
  max-width: none !important;
}
map, div[map] {
  display : block; 
  opacity : 1;
  position: absolute;
  top     : 0px;
  right   : 0px; 
  width   : 100%; 
  height  : 100%;
}

.logo{
  z-index:10;
  padding:15px;
} 

.logoBg{
  background-color:rgba(255,255,255,0.85);
  border-radius: 80px;
}

table{
  border-radius: 5px; 
}

.withfadein {
  transition: all 60s ease-in;
}

.withfadeout {
  transition: all 60s ease-out;
}

